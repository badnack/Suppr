<div class="host_suppr">
    <%= link_to '<button type="button" class="btn btn-primary btn-lg">Host a Suppr</button>'.html_safe, new_dinner_path %>
 </div>

<h1>Upcoming Supprs</h1>
<p>Times are local to the event.</p>
<% if notice %>
    <p id="notice"><%= notice %></p>
<% end %>

<table id="list_dinners">
  <tbody class="page">
    <%= render @dinners %>
  </tbody>
</table>

<%= paginate @dinners %>
